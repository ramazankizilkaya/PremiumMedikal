<!--Footer Top Categories Component-->

@model IEnumerable<FooterTopCategoryDto>

@{
    Layout = "~/Views/Shared/_ComponentLayout.cshtml";
}

    <li>
        <h3><a href="javascript:void(0);" target="_self">@ViewBag.baslik</a></h3>

        <ul class="list-unstyled">

            @foreach (var item in Model)
            {
                <li><a href="@item.TargetHref" target="_self">@item.Name</a></li>
            }

        </ul>

    </li>